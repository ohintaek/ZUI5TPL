<mvc:View
	controllerName="com.ui5.echoit.temp.tables.sapMTable.VWMTB"
	xmlns="sap.m"
	xmlns:ux="sap.uxap"
	xmlns:html="http://www.w3.org/1999/xhtml"
	xmlns:mvc="sap.ui.core.mvc">
	<Page
		title="sap.m.Table"
		showHeader="true"
		class="sapUiContentPadding"
		width="auto"
		showNavButton="true"
		navButtonPress="onNavBack">
		<ux:ObjectPageLayout id="ObjectPageLayout">
			<ux:headerTitle>
				<ux:ObjectPageHeader objectTitle="JSON Model Binding"/>
			</ux:headerTitle>
			<ux:sections>
				<ux:ObjectPageSection>
					<ux:subSections>
						<ux:ObjectPageSubSection >
							<ux:blocks>
								<Table
									id="mtable"
									mode="MultiSelect"
									fixedLayout="false"
									backgroundDesign="Transparent">
									<columns>
										<Column
											hAlign="Center">
											<Text text="Airline Code" />
										</Column>
										<Column
											hAlign="Center">
											<Text text="Flight Number" />
										</Column>
										<Column
											hAlign="Center">
											<Text text="Flight date" />
										</Column>
										<Column
											hAlign="Center">
											<Text text="Price" />
										</Column>
										<Column
											hAlign="Center">
											<Text text="Currency" />
										</Column>
										<Column
											hAlign="Center">
											<Text text="Plane Type" />
										</Column>
										<Column
											hAlign="Center">
											<Text text="Seatsmax" />
										</Column>
										<Column
											hAlign="Center">
											<Text text="Seatsocc" />
										</Column>
										<Column
											hAlign="Center">
											<Text text="PaymentSum" />
										</Column>
									</columns>
								</Table>
							</ux:blocks>
						</ux:ObjectPageSubSection>
					</ux:subSections>
				</ux:ObjectPageSection>
			</ux:sections>
		</ux:ObjectPageLayout>
	</Page>
</mvc:View>