<mvc:View
	controllerName="com.ui5.echoit.temp.test.TestPage"
	xmlns="sap.m"
	xmlns:mvc="sap.ui.core.mvc"
	id="TestPage">
	<Page
		title="Test Page"
		showHeader="true"
		class="sapUiContentPadding"
		width="auto"
		showNavButton="false"
		navButtonPress="onNavBack">

		<ScrollContainer>
			<StandardTile 
				id="noticeTile"
				icon="sap-icon://notification-2"
				info="Information"
				number="11"
				type="Monitor"
				title="공지사항"
				press="onPressNotice"/>
			
			<StandardTile
				id="Split1"
				icon="sap-icon://notification-2"
				info="Information"
				number=""
				type="Monitor"
				title="Split Container Test"
				press="onPressTile1"/>
				
			<StandardTile
				id="Tile2"
				icon="sap-icon://notification-2"
				info="Information"
				number=""
				type="Monitor"
				title="Tile2"
				press="onPressTile2"/>
		</ScrollContainer>
	</Page>
</mvc:View>
