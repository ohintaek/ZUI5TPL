{
  "_version": "1.4.0",
  "sap.app": {
	"id": "com.ui5.yoon",
	"type": "application",
	"title": "Yoon UI5 Portal",
	"description": "Yoon UI5 Portal",
	"applicationVersion": {
	  "version": "1.0.0"
	}
  },
  "sap.ui": {
	"technology": "UI5",
	"deviceTypes": {
		"desktop": true,
		"tablet": true,
		"phone": true
	},
	"supportedThemes" : [ "sap_belize" ]
  },
  "sap.ui5": {
	"rootView": {
		"viewName": "com.ui5.yoon.App",
		"type": "XML",
		"async": true,
		"id": "app"
	},
	"dependencies": {
	  "minUI5Version": "1.44",
	  "libs": {
		"sap.m" : { "minVersion" : "1.40.0", "lazy" : "true"}
	  }
	},
	
	"models": {
	  "i18n": {
		"type": "sap.ui.model.resource.ResourceModel",
		"settings": {
		  "bundleName": "com.ui5.yoon.i18n.i18n"
		}
	  }
	},
	"routing": {
	  "config": {
		"routerClass": "sap.m.routing.Router",
		"viewType": "XML",
		"viewPath": "com.ui5.yoon",
		"controlId": "app",
		"clearTarget" : "true",
		"controlAggregation": "pages"
	  },
	  "routes": [
		{ "name": "main", "target" : "main", "pattern": "" }
		
		
	  ],
	  "targets": {
	  	"main": {"viewName": "App"}
	  }
	}
  }
 }
