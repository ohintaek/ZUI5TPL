<mvc:View
	controllerName="com.ui5.yoon.temp.tables.sapMTable.VWMTB"
	xmlns="sap.m"
	xmlns:ux="sap.uxap"
	xmlns:html="http://www.w3.org/1999/xhtml"
	xmlns:mvc="sap.ui.core.mvc">
<Page
	title="sap.m.Table"
	showHeader="true"
	class="sapUiContentPadding"
	width="auto"
	showNavButton="true"
	navButtonPress="onNavBack">
	
	<ux:ObjectPageLayout id="ObjectPageLayout">
		<ux:headerTitle>
			<ux:ObjectPageHeader objectTitle="Single View"/>
		</ux:headerTitle>

		<ux:sections>
			<ux:ObjectPageSection title="example">
				<ux:subSections>

					<ux:ObjectPageSubSection title="Example">
						<ux:blocks>
							<Table
								id="mtable"
								mode="MultiSelect"
								fixedLayout="false"
								backgroundDesign="Transparent"
								PopinLayout="GridLarge">
								<columns>
									<Column
										hAlign="Center">
										<Text text="name" />
									</Column>
									<Column
										hAlign="Center">
										<Text text="productId" />
									</Column>
									<Column
										hAlign="Center">
										<Text text="lastPurchase" />
									</Column>
									<Column
										hAlign="Center">
										<Text text="payment" />
									</Column>
									<Column
										hAlign="Center">
										<Text text="state" />
									</Column>
									<Column
										hAlign="Center">
										<Text text="amount" />
									</Column>
									<Column
										hAlign="Center">
										<Text text="currencyCode" />
									</Column>
								</columns>
							</Table>
						</ux:blocks>
					</ux:ObjectPageSubSection>
				</ux:subSections>
			</ux:ObjectPageSection>
		</ux:sections>
	</ux:ObjectPageLayout>
</Page>



</mvc:View>
